const { Chip, Line } = require('node-libgpiod');

const chip = new Chip(0);           // Chip 0 (depends on your Pi model - Marshal Pi3B )
const led = new Line(chip, 23);     // GPIO23

led.requestOutputMode();

let count = 10;
const blink = () => {
  if (count > 0) {
    led.setValue(count % 2);
    count--;
    setTimeout(blink, 1000);
  } else {
    led.setValue(0);
    
  }
};

blink();

// not add code for turn out the led yet
